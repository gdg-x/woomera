{
  "hosting": [
    {
      "predeploy": "sh ./deploy.sh",
      "target": "woomera",
      "public": "dist/woomera",
      "ignore": [
        "index.html",
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "universal"
        }
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "functions"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
